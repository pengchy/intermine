<project type="bio">
  <property name="target.model" value="genomic"/>
  <property name="source.location" location="/home/pengchy/soft/intermine/bio/sources/"/>
  <property name="common.os.prefix" value="common"/>
  <property name="intermine.properties.file" value="testmine.properties"/>
  <property name="default.intermine.properties.file" location="/home/pengchy/soft/intermine/testmine/default.intermine.integrate.properties"/>

  <sources>
    <source name="locust-gff" type="gff">
      <property name="gff3.taxonId" value="7004"/>
      <property name="gff3.seqDataSourceName" value="LocustGenomeProject"/>
      <property name="gff3.dataSourceName" value="LocustGenomeProject"/>
      <property name="gff3.seqClsName" value="Chromosome"/>
      <property name="gff3.dataSetTitle" value="Locust L. migratoria genome"/>
      <property name="src.data.dir" location="/home/pengchy/Project/LocustMine/testdata/genome/gff"/>
    </source>

    <source name="locust-chromosome-fasta" type="fasta" dump="true">
      <property name="fasta.className" value="org.intermine.model.bio.Chromosome"/>
      <property name="fasta.dataSourceName" value="LocustGenomeProject"/>
      <property name="fasta.dataSetTitle" value="Locust chromosome sequence"/>
      <property name="fasta.taxonId" value="7004"/>
      <property name="fasta.includes" value="Locust.genome.2.4.1.fa"/>
      <property name="src.data.dir" location="/home/pengchy/Project/LocustMine/testdata/genome/fasta"/>
    </source>

    <source name="locust-cds-fasta" type="fasta" dump="true">
      <property name="fasta.className" value="org.intermine.model.bio.CDS"/>
      <property name="fasta.dataSourceName" value="LocustGenomeProject"/>
      <property name="fasta.dataSetTitle" value="Locust CDS sequences"/>
      <property name="fasta.taxonId" value="7004"/>
      <property name="fasta.includes" value="Locust.V2.4.1.cds"/>
      <property name="src.data.dir" location="/home/pengchy/Project/LocustMine/testdata/genome/cds"/>
    </source>

    <source name="locust-protein-fasta" type="fasta" dump="true">
      <property name="fasta.className" value="org.intermine.model.bio.Protein"/>
      <property name="fasta.sequenceType" value="protein"/>
      <property name="fasta.dataSourceName" value="LocustGenomeProject"/>
      <property name="fasta.dataSetTitle" value="Locust protein sequences"/>
      <property name="fasta.taxonId" value="7004"/>
      <property name="fasta.includes" value="Locust.V2.4.1.pep"/>
      <property name="src.data.dir" location="/home/pengchy/Project/LocustMine/testdata/genome/protein"/>
    </source>

    <source name="go" type="go" dump="true">
      <property name="src.data.file" location="/home/pengchy/Project/LocustMine/testdata/go/go-basic.obo"/>
			<property name="createrelations" value="false"/>
    </source>

    <source name="go-annotation" type="go-annotation" dump="true">
      <property name="ontologyfile" location="/home/pengchy/Project/LocustMine/testdata/go/go-basic.obo"/>
      <property name="src.data.dir" location="/home/pengchy/Project/LocustMine/testdata/go-annotation/"/>
    </source>

    <source name="interpro" type="interpro" dump="true">
      <property name="src.data.dir" location="/home/pengchy/Project/LocustMine/testdata/interpro/"/>
    </source>

		<source name="protein2ipr" type="protein2ipr" dump="true">
			<property name="src.data.dir" location="/home/pengchy/Project/LocustMine/testdata/protein2ipr/"/>
			<property name="src.data.dir.includes" value="Locust.V2.4.1.protein2ipr"/>
			<property name="protein2ipr.organisms" value="7004"/>
		</source>

