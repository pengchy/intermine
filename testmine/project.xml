    <source name="update-publications" type="update-publications">
      <property name="src.data.file" location="/home/pengchy/soft/intermine/testmine/integrate/build/publications.xml"/>
    </source>
    <source name="entrez-organism" type="entrez-organism">
      <property name="src.data.file" location="/home/pengchy/soft/intermine/testmine/integrate/build/organisms.xml"/>
    </source>
